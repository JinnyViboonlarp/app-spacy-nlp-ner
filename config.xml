<tool id="spacy" name="Spacy NLP" version="0.0.3">
  <description>
    Apply Spacy NLP to all text documents in an input MMIF file.
  </description>
  <command interpreter="python3">test.py $input $output</command>
  <inputs>
      <param name="input" type="data" format="json" label="Input Text"/>
  </inputs>
  <outputs>
    <data name="output" format="json" label="Spacy annotations"/>
  </outputs>
  <help>Text</help>
</tool>
